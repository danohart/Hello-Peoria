enum Permission {
  ADMIN
  USER
  PLACECREATE
  PLACEUPDATE
  PLACEDELETE
  PERMISSIONUPDATE
}

type User {
  id: ID! @id
  firstName: String!
  lastName: String!
  email: String! @unique
  password: String!
  resetToken: String
  resetTokenExpiry: Float
  permissions: [Permission] @scalarList(strategy: RELATION)
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type Place {
  id: ID! @id
  name: String!
  address: String!
  description: String!
  image: String
  largeImage: String
  category: String
  place_id: String
  paths: String
  tags: String
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type Event {
  id: ID! @id
  data: Json
}

type FsqInfo {
  id: ID! @id
  data: Json
}