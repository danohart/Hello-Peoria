type User {
  id: ID! @unique
  name: String!
  email: String!
}

type Place {
  id: ID! @unique
  name: String!
  address: String!
  description: String!
  image: String
  largeImage: String
  category: String
  path: Path
  place_id: String
}

type Path {
  mainPath: String
  secondaryPath: [String!]
}